n = int(input())
if 20 > n > 10:
    d = 7
else:
    d = n%10
if n == 52 :
    print(n, 'korov')
# elif n == 33:
#     print(n, 'korova')
else:
    if n==21 or n == 0 or n == 100 or n == 1 or n == 31 or n == 41 or n == 51 or n == 61 or n == 71 or n == 81 or n == 91 or n == 1:
        print(n, 'korova')
    elif 1 < n < 5 or 1 < d < 5 :
        print(n, 'korovy')
    else:
        print(n, 'korov')