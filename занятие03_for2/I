a, b = map(int,input().split())
d = 0 #7
s = 0 #13
for i in range(a, b+1):
    if i % 7 == 0:
        d = 1
        break
 
for i in range(a, b+1):
    if i % 13 == 0:
        s = 1
 
 
if d == s == 1:
    print('Yes Yes ')
elif d == s == 0:
    print('No No ')
elif d == 1 and s == 0:
    print('Yes No ')
else:
    print('No Yes')